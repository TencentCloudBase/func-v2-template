{
  "functionsRoot": ".",
  "functions": [
    {
      "name": "default",
      "directory": "./cloudrunfunctions",
      "target": "main",
      "triggerPath": "/"
    },
    {
      "name": "echo",
      "directory": "./cloudrunfunctions/echo",
      "source": "echo.js",
      "triggerPath": "/echo"
    },
    {
      "name": "sum",
      "directory": "./cloudrunfunctions/sum",
      "triggerPath": "/sum"
    },
    {
      "name": "ws",
      "directory": "./cloudrunfunctions/ws",
      "triggerPath": "/ws"
    },
    {
      "name": "sse",
      "directory": "./cloudrunfunctions/sse",
      "triggerPath": "/sse"
    }
  ],
  "routes": [
    {
      "functionName": "echo",
      "path": "/call-echo"
    }
  ]
}
