{
  "functionsRoot": "./cloudrunfunctions",
  "functions": [
    {
      "name": "default",
      "directory": ".",
      "target": "main",
      "triggerPath": "/"
    },
    {
      "name": "echo",
      "directory": "echo",
      "source": "echo.js",
      "triggerPath": "/echo"
    },
    {
      "name": "ws",
      "directory": "ws",
      "triggerPath": "/ws"
    },
    {
      "name": "sse",
      "directory": "sse",
      "triggerPath": "/sse"
    },
    {
      "name": "sse-openai",
      "directory": "sse-openai",
      "triggerPath": "/sse-openai"
    }
  ],
  "routes": [
    {
      "functionName": "echo",
      "path": "/call-echo"
    }
  ]
}
